pipeline {
    agent any

    stages {
        stage('Build jenkins1') {
            steps {
                script {
                    build job: 'GitHub jenkins/jenkins1/development'
                }
            }
        }

        stage('Build jenkins2') {
            steps {
                script {
                    build job: 'GitHub jenkins/jenkins2/development'
                }
            }
        }
    }
}
